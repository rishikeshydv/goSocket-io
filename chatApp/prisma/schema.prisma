generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model chatHistory {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  /// Could not determine type: the field only had null or empty values in the sample set.
  fullChat Json?
  roomId   String
}

model transactions {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
