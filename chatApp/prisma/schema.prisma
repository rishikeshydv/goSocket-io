generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model chatHistory {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  fullChat String[]
  roomdId  String
}

model transactions {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
